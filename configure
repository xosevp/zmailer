  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :

	ac_cv_func_maillock_lmail=yes
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    LIBS="$t_oldLibs"
    if test "x$ac_cv_func_maillock_lmail" = "x"; then
      # On some Debian systems this exists as  -llockfile  :-/
      LIBS="$LIBS -llockfile"
      cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <maillock.h>
int
main ()
{
mailunlock();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :

		ac_cv_func_maillock_llockfile=yes
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
      LIBS="$t_oldLibs"
    fi
  else
    ac_cv_func_maillock_lmail=no
    ac_cv_func_maillock_llockfile=no
  fi
fi
if test "$ac_cv_func_maillock_lmail" = "yes"; then

$as_echo "#define HAVE_MAILLOCK 1" >>confdefs.h

	LIBMAIL="-lmail"
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: System has  maillock()  with -lmail" >&5
$as_echo "System has  maillock()  with -lmail" >&6; }
else
  if test "$ac_cv_func_maillock_llockfile" = "yes"; then

$as_echo "#define HAVE_MAILLOCK 1" >>confdefs.h

	LIBMAIL="-llockfile"
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: System has  maillock()  with -llockfile" >&5
$as_echo "System has  maillock()  with -llockfile" >&6; }
  else

$as_echo "#define HAVE_DOTLOCK 1" >>confdefs.h

	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: Using traditional UNIX 'dot-lock' mailbox locks" >&5
$as_echo "Using traditional UNIX 'dot-lock' mailbox locks" >&6; }
  fi
fi
else

$as_echo "#define HAVE_DOTLOCK 1" >>confdefs.h

fi


